{
  "name": "chemem/bingo-functional-repl",
  "type": "library",
  "description": "A simple REPL for the bingo-functional library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Lochemem Bruno Michael",
      "email": "lochbm@live.com"
    }
  ],
  "require": {
    "ext-apcu": "~5",
    "chemem/bingo-functional": "~1",
    "clue/stdio-react": "~2",
    "mmarica/display-table": "~1",
    "nikic/php-parser": "~4",
    "php-parallel-lint/php-console-color": "~0",
    "react/child-process": "~0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "~2",
    "giorgiosironi/eris": "~0",
    "phpunit/phpunit": "~8"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Chemem\\Bingo\\Functional\\Repl\\": "src/"
    },
    "files": [
      "src/Printer/functions.php",
      "src/Parser/functions.php",
      "src/functions.php",
      "src/constants.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Chemem\\Bingo\\Functional\\Repl\\Tests\\": "tests/"
    },
    "files": [
      "tests/functions.php"
    ]
  },
  "bin": [
    "bfrepl"
  ],
  "scripts": {
    "cs:fix": "php-cs-fixer fix --config=.php_cs --diff --verbose --allow-risky=yes",
    "cs:fix-dry": "php-cs-fixer fix --dry-run --config=.php_cs --diff --verbose --allow-risky=yes",
    "test": "phpunit -c phpunit.xml.dist"
  }
}
