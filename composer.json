{
    "name": "chemem/bingo-functional-repl",
    "type": "library",
    "description": "A simple REPL for the bingo-functional library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Lochemem Bruno Michael",
            "email": "lochbm@live.com"
        }
    ],
    "require": {
        "ext-apcu": "~5.1",
        "chemem/bingo-functional": "~1.12",
        "clue/stdio-react": "~2",
        "jakub-onderka/php-console-color": "~0",
        "mmarica/display-table": "~1.0.0",
        "nikic/php-parser": "~4",
        "react/child-process": "~0.6"
    },
    "require-dev": {
        "giorgiosironi/eris": "~0",
        "phpunit/phpunit": "~8",
        "squizlabs/php_codesniffer": "~3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Chemem\\Bingo\\Functional\\Repl\\": "src/"
        },
        "files": [
            "src/Printer/functions.php",
            "src/Parser/constants.php",
            "src/Parser/functions.php",
            "src/Repl/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Chemem\\Bingo\\Functional\\Repl\\Tests\\": "tests/"
        },
        "files": [
            "tests/functions.php"
        ]
    },
    "minimum-stability": "stable",
    "bin": [
        "bfrepl"
    ],
    "scripts": {
        "cs": "phpcbf --extensions=php --standard=PSR12",
        "test": "phpunit -c phpunit.xml.dist"
    }
}
